	pages = {199--215},
	file = {Schmitter et al. - 2017 - Smooth shapes with spherical topology Beyond trad.pdf:/Users/pradaty/Zotero/storage/R4W6FPPM/Schmitter et al. - 2017 - Smooth shapes with spherical topology Beyond trad.pdf:application/pdf}
